#. Example: "1 hour 10 minutes to charge"
#: ../src/device.c:681
#, c-format
msgid "%d %s %d %s to charge"
msgstr "noch %d %s %d %s zu laden"

#: ../src/device.c:682 ../src/device.c:700
msgid "hour"
msgid_plural "hours"
msgstr[0] "Stunde"
msgstr[1] "Stunden"

